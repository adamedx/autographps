              <TableColumnItem>
                <ScriptBlock>$::.MetaGraphFormatter.MatchedSearchTerms($_, 'TypeId')</ScriptBlock>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Criteria</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>$::.MetaGraphFormatter.MatchedSearchTerms($_, $null)</ScriptBlock>
              </TableColumnItem>
