      </GroupBy>

      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <Label>AuthType</Label>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>ConsentType</Label>
          </TableColumnHeader>
